<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cocoding">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SignupActivity"></activity>

        <activity android:name=".MainActivity">

        </activity>

        <activity
            android:name=".IdeaActivity"
            android:label="@string/app_name" android:parentActivityName=".MainActivity">

        </activity>

        <activity android:name=".FlowActivity" android:label="@string/app_name" android:parentActivityName=".MainActivity"/>

        <activity android:name=".ObjectActivity" android:label="@string/app_name" android:parentActivityName=".MainActivity"/>

        <activity android:name=".CodeActivity" android:label="@string/app_name" android:parentActivityName=".MainActivity"/>

        <activity android:name=".Object.Object_List"/>

        <activity android:name=".Idea.Idea_Topic" android:parentActivityName=".IdeaActivity"/>
        <activity android:name=".Idea.Idea_Topic_Content" android:parentActivityName=".IdeaActivity"/>
        <activity android:name=".Idea.Idea_Algorithm" android:parentActivityName=".IdeaActivity"/>

    </application>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

</manifest>